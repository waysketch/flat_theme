{
  "name": "flat_theme",
  "version": "1.0.0",
  "description": "This is a theme using create-react-app",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "prod": "node server.js",
    "heroku-postbuild": "npm run build",
    "seed": "node utils/scripts/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waysketch/flat_theme.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/waysketch/flat_theme/issues"
  },
  "homepage": "https://github.com/waysketch/flat_theme#readme",
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.11.16",
    "nodemailer": "^6.4.18",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
