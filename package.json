{
  "name": "flat_theme",
  "version": "1.0.0",
  "description": "This is a theme using create-react-app",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "prod": "node server.js",
    "heroku-postbuild": "npm run build",
    "seed": "node utils/scripts/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waysketch/flat_theme.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/waysketch/flat_theme/issues"
  },
  "homepage": "https://github.com/waysketch/flat_theme#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "path": "^0.12.7"
  }
}
